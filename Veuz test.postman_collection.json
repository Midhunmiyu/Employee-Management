							"script": {
								"exec": [
									"// Parse the JSON response\r",
									"var jsonData = pm.response.json();\r",
									"\r",
									"// Assuming the token is in the `token` field of the response\r",
									"var token = jsonData[\"access_token\"];\r",
									"\r",
									"// Save the token to an environment variable\r",
									"pm.environment.set(\"token\", token);\r",
									"\r",
									"// Log the token to the console (optional)\r",
									"console.log(\"Token saved to environment variable:\", token);"
